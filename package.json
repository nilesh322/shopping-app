{
  "name": "shoping-cart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "materialize-css": "^1.0.0-rc.2",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {},
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start",
    "start": "set HTTPS=true && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/scss -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o public/css --watch --recursive"
  }
}
